{
  "extends": "tslint-config-airbnb",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": false,
    "align": false,
    "max-line-length": [
      true,
      120
    ],
    "no-trailing-whitespace": true,
    "no-debugger": true,
    "no-console": true,
    "import-name": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "case-insensitive",
        "named-imports-order": "case-insensitive"
      }
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ]
  }
}
