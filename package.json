{
  "name": "wolk-rest",
  "version": "0.0.1",
  "description": "A client for interacting with WolkAbout IoT Platform's API",
  "main": "dist/wolk-rest.umd.js",
  "module": "dist/wolk-rest.es5.js",
  "typings": "dist/types/wolk-rest.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --module commonjs",
    "prebuild": "npm run clean && npm run compile",
    "build": "rollup -c",
    "docs": "typedoc --name 'WolkREST client library' --out ./docs/ ./src/ --target ES6 --module commonjs --hideGenerator",
    "test": "jest --runInBand",
    "prepush": "npm test",
    "pretest": "npm run compile && npm run lint",
    "lint": "tslint --project ./tsconfig.json",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolkabout/wolk-rest"
  },
  "author": "WolkAbout",
  "contributors": [
    {
      "name": "Mladen Ruzicic",
      "email": "mladen.ruzicic@wolkabout.com",
      "url": "http://github.com/cedevita"
    },
    {
      "name": "Radovan Skendzic",
      "email": "radovan.skendzic@wolkabout.com",
      "url": "http://github.com/rskendzic"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.5.5",
    "@types/qs": "^6.5.1",
    "coffeescript": "^2.3.1",
    "dotenv": "^6.0.0",
    "husky": "^0.14.3",
    "istanbul-reporter-badges": "^1.0.2",
    "jest": "^23.5.0",
    "lodash.camelcase": "^4.3.0",
    "nyc": "^12.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.5",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.16.1",
    "source-map-support": "^0.5.8",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "typedoc": "^0.11.1",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "qs": "^6.5.2"
  },
  "engines": {
    "node": ">=10.8.0",
    "npm": ">=6.2.0"
  }
}
